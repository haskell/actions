{
  "name": "@rwe/actions-hlint-setup",
  "version": "1.0.1",
  "description": "GitHub Action: Set up hlint",
  "main": "dist/index.js",
  "scripts": {
    "lint": "true",
    "test": "true",
    "compile": "tsc",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run lint && npm run compile && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haskell/actions.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Haskell",
    "Lint",
    "HLint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haskell/actions/issues"
  },
  "homepage": "https://github.com/haskell/actions#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vercel/ncc": "^0.36.1",
    "typescript": "^5.0.3"
  }
}
