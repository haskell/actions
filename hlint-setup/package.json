{
  "name": "@rwe/actions-hlint-setup",
  "version": "1.0.1",
  "description": "GitHub Action: Set up hlint",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup && npm run generate-licenses",
    "dev": "tsup --watch",
    "lint": "eslint ./src --ext ts",
    "generate-licenses": "npx yarn licenses generate-disclaimer --prod >> dist/licenses.txt",
    "all": "npm run build && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haskell/actions.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Haskell",
    "Lint",
    "HLint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haskell/actions/issues"
  },
  "homepage": "https://github.com/haskell/actions#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-github": "^4.7.0",
    "tsup": "^6.7.0",
    "@types/node": "^18.16.3",
    "@vercel/ncc": "^0.36.1",
    "typescript": "^5.0.4"
  }
}
