{
  "name": "setup-haskell",
  "version": "2.3.6",
  "private": true,
  "description": "setup haskell action",
  "main": "lib/setup-haskell",
  "types": "lib/setup-haskell",
  "scripts": {
    "build": "tsup && npm run generate-licenses",
    "dev": "tsup --watch",
    "lint": "eslint ./src --ext ts",
    "generate-licenses": "npx yarn licenses generate-disclaimer --prod >> dist/licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haskell/actions.git"
  },
  "keywords": [
    "actions",
    "haskell",
    "ghc",
    "cabal",
    "setup"
  ],
  "author": "jared-w",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "compare-versions": "^5.0.3",
    "ensure-error": "^4.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.59.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
