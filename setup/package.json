{
  "name": "setup-haskell",
  "version": "1.2.2",
  "private": true,
  "description": "setup haskell action",
  "main": "lib/setup-haskell",
  "types": "lib/setup-haskell",
  "scripts": {
    "bundle": "ncc build src/main.ts  && tsc -p tsconfig-lib.json",
    "test": "jest",
    "prepare": "cd .. && husky install setup/.husky",
    "yolo-upgrade": "npx npm-check-updates -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haskell/actions.git"
  },
  "keywords": [
    "actions",
    "haskell",
    "ghc",
    "cabal",
    "setup"
  ],
  "author": "jared-w",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "compare-versions": "^5.0.3",
    "ensure-error": "^4.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.1",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.32.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
